{
  "sections": {
    "page-title": {
      "type": "page-title",
      "blocks": {
        "title": {
          "type": "heading",
          "settings": {
            "title": "{{ page.title }}"
          }
        }
      },
      "block_order": [
        "title"
      ],
      "settings": {
      }
    },
    "our_picture": {
      "type": "image-banner",
      "custom_css": [
        "{margin-left: auto; margin-right: auto; max-width: 1356px;}",
        "@media (min-width: 1356px) {{border-radius: 8px; overflow: clip;}}"
      ],
      "settings": {
        "image_height": "adapt",
        "full_width": true
      }
    },
    "about_us": {
      "type": "rich-text",
      "blocks": {
        "content": {
          "type": "text",
          "settings": {
          }
        }
      },
      "block_order": [
        "content"
      ],
      "custom_css": [
        ".rte { color: var(--color-foreground-55); font-size: 1.25rem;}",
        "@media (min-width: 768px) {.rte {font-size: 1.5rem; line-height: 150%;}}"
      ],
      "settings": {
        "color_scheme": "background-1"
      }
    },
    "our_story_1": {
      "type": "image-with-text",
      "blocks": {
        "heading": {
          "type": "heading",
